  "betterScripts": {
    "start:uat": {
      "command": "yarn start",
      "env": {
        "DEBUG": "true",
        "NODE_ENV": "uat"
      }
    },
    "start:prod": {
      "command": "yarn start",
      "env": {
        "DEBUG": "true",
        "NODE_ENV": "production"
      }
    },
    "build:sit": {
      "command": "yarn build",
      "env": {
        "NODE_ENV": "sit"
      }
    },
    "build:uat": {
      "command": "yarn build",
      "env": {
        "NODE_ENV": "uat"
      }
    },
    "build:prod": {
      "command": "yarn build",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "standard-version": {
    "scripts": {
      "postbump": "yarn update-version-snapshot"
    }
  },
  "scripts": {
    "postinstall": "husky install",
    "project-version": "standard-version -a",
    "update-version-snapshot": "jest components/Html --updateSnapshot && git add .",
    "lint-js": "eslint --cache --ignore-path .eslintignore --ignore-pattern \"!**/.*\" .",
    "lint-js-timing": "TIMING=1 eslint --cache --ignore-path .eslintignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint --cache \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix-js": "yarn run lint-js --fix",
    "fix-css": "yarn run lint-css --fix",
    "fix": "yarn run fix-js && yarn run fix-css",
    "flow:check": "flow check",
    "test": "jest --no-cache",
    "test-watch": "yarn run test --watch --notify",
    "test-cover": "yarn run test --coverage",
    "test-ci": "yarn run test-cover --ci",
    "test-update": "yarn test --updateSnapshot",
    "coverage": "yarn run test-cover && open coverage/lcov-report/index.html",
    "clean": "babel-node tools/run clean",
    "copy": "babel-node tools/run copy",
    "bundle": "babel-node tools/run bundle",
    "build": "babel-node tools/run build",
    "build-sit": "better-npm-run build:sit",
    "build-uat": "better-npm-run build:uat",
    "build-prod": "better-npm-run build:prod",
    "build-stats": "yarn build-prod --analyse",
    "clean-cache": "babel-node tools/run clean-cache",
    "start": "babel-node tools/run start",
    "start-uat": "better-npm-run start:uat",
    "start-prod": "better-npm-run start:prod",
    "storybook": "start-storybook -p 60078",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages"
  }
